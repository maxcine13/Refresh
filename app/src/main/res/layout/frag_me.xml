                        android:scaleType="fitXY"
                        android:src="@drawable/me_change" />

                    <ImageView
                        android:id="@+id/arrow_pw_change"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/me_arrow_right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toLeftOf="@+id/arrow_pw_change"
                        android:layout_toRightOf="@+id/ic_pw_change"
                        android:text="@string/me_pw_change"
                        android:textColor="@color/Black_transparent_black_percent_60"
                        android:textSize="16sp" />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.2dp"
                    android:background="@color/Black_transparent_black_percent_15" />

                <!--客服栏目-->
                <RelativeLayout
                    android:id="@+id/bt_customer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_white_base_5"
                    android:paddingBottom="15dp"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:paddingTop="15dp">

                    <ImageView
